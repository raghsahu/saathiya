                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_marginTop="@dimen/_5sdp"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <com.prometteur.sathiya.utills.TextViewCustomFont
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="@string/diet"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_12sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        />
                    <com.prometteur.sathiya.utills.TextViewCustomFont
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:layout_marginRight="@dimen/_10sdp"
                        android:text="*"
                        android:textColor="@color/red"
                        android:textSize="@dimen/_12sdp" />
                </LinearLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.prometteur.sathiya.utills.TextViewCustomFont
                        android:id="@+id/tvDiet"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/please_select"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:textColor="@color/colorDarkGray"
                        android:drawableRight="@drawable/icn_blck_down"
                        android:background="@drawable/bg_corner_round_greyfill"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:padding="@dimen/_10sdp"/>
                </FrameLayout>
            </LinearLayout>
            <com.prometteur.sathiya.utills.TextViewCustomFont
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_weight="1"
                android:text="@string/mobile_number_parent"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp" />

            <com.prometteur.sathiya.utills.CustomeEditText
                android:id="@+id/edtMobileParent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:inputType="phone"
                android:background="@drawable/bg_corner_round_greyfill"
                android:padding="@dimen/_10sdp"
                android:hint="@string/enter_here"
                android:text=""
                android:digits="1234567890"
                android:maxLength="10"
                android:textColor="@color/black"
                style="@style/EditTextviewStyle"/>

            <com.prometteur.sathiya.utills.TextViewCustomFont
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_weight="1"
                android:text="@string/income"
                android:textColor="@color/black"
                android:textSize="@dimen/_13sdp"
                android:visibility="gone"/>

            <com.appyvet.materialrangebar.RangeBar
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:mrb_barWeight="3dp"
                app:mrb_rangeBarColor="@color/lightpink"
                app:mrb_leftThumbColor="@color/lightpink"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                app:mrb_thumbSize="10dp"
                android:visibility="gone"
                />
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
            <com.prometteur.sathiya.utills.TextViewCustomFont
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_1sdp"
                android:layout_weight="1"
                android:text="@string/shadi_budget"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp" />
            <com.prometteur.sathiya.utills.TextViewCustomFont
                android:id="@+id/tvShowBudget"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_1sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_weight="1"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/_13sdp" />
        </LinearLayout>
