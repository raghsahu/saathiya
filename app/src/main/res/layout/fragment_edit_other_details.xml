                android:id="@+id/rangebarShadiBgt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:mrb_barWeight="3dp"
                app:mrb_rangeBarColor="@color/lightpink"
                app:mrb_leftThumbColor="@color/colorAccent"
